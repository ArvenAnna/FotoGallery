{
  "name": "gallery",
  "version": "1.0.0",
  "description": "foto gallery",
  "main": "webpack.config.js",
  "bin": {
    "foto": "app.bundle.js"
  },
  "scripts": {
    "test": "NODE_ENV=test jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "dev-server": "node dev-server.js",
    "dev-client": "webpack-dev-server --port 3000",
    "server": "node --max-old-space-size=4192 server/server.js localhost",
    "docker-server": "node --max-old-space-size=4192 server/server.js mongo"
  },
  "jest": {
    "setupFiles": [
      "./test/jestsetup.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|scss|less)$": "identity-obj-proxy",
      "\\.(svg|jpg|png)$": "<rootDir>/empty-module.js"
    },
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.jsx$": "babel-jest"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ArvenAnna/FotoGallery"
  },
  "keywords": [
    "gallery"
  ],
  "author": "Anna",
  "license": "ISC",
  "dependencies": {
    "axios": "0.17.1",
    "body-parser": "1.18.2",
    "express": "4.16.2",
    "express-fileupload": "0.4.0",
    "express-logging": "1.1.1",
    "fs.promised": "3.0.0",
    "logops": "2.1.0",
    "mongoose": "5.0.3",
    "mongoose-paginate": "5.0.3",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-redux": "5.0.6",
    "react-router": "4.2.0",
    "react-router-dom": "4.2.2",
    "redux": "3.7.2",
    "redux-connect-decorator": "0.2.0",
    "redux-logger": "3.0.6",
    "redux-thunk": "2.2.0",
    "react-custom-scrollbars": "4.2.1",
    "react-loaders": "3.0.1",
    "loaders.css": "0.1.2",
    "react-paginate": "5.0.0",
    "react-s-alert": "1.4.1",
    "styled-components": "3.1.2",
    "google-maps-react": "1.1.2"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-jest": "^22.1.0",
    "babel-loader": "7.1.2",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "css-loader": "0.28.9",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.1",
    "file-loader": "1.1.6",
    "identity-obj-proxy": "^3.0.0",
    "image-webpack-loader": "4.0.0",
    "jest": "^22.1.4",
    "js-to-styles-var-loader": "1.1.0",
    "less": "2.7.3",
    "less-loader": "4.0.5",
    "node-sass": "4.7.2",
    "postcss-loader": "2.1.0",
    "react-svg-loader": "2.1.0",
    "sass-loader": "6.0.6",
    "source-map-loader": "0.2.3",
    "style-loader": "0.20.1",
    "url-loader": "0.6.2",
    "webpack": "3.10.0",
    "webpack-dev-server": "2.11.1"
  }
}
